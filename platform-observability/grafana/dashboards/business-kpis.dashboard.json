{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "description": "",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "8.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Total User Sign-ups",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 1,
      "gridPos": { "h": 6, "w": 8, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum(app_user_signups_total)",
          "legendFormat": "Total Users",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "text": { "valueSize": 40 },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 } }
    },
    {
      "title": "Total Posts Created",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 2,
      "gridPos": { "h": 6, "w": 8, "x": 8, "y": 0 },
      "targets": [
        {
          "expr": "sum(app_posts_created_total)",
          "legendFormat": "Total Posts",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "text": { "valueSize": 40 },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 } }
    },
    {
      "title": "Currently Active Users (DAU estimate)",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 3,
      "gridPos": { "h": 6, "w": 8, "x": 16, "y": 0 },
      "targets": [
        {
          "expr": "sum(app_active_users)",
          "legendFormat": "Active Users",
          "refId": "A"
        }
      ],
      "options": {
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "text": { "valueSize": 40 },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "fieldConfig": { "defaults": { "unit": "short", "decimals": 0 } }
    },
    {
      "title": "User Sign-ups Over Time",
      "type": "timeseries",
      "datasource": "Prometheus",
      "id": 4,
      "gridPos": { "h": 10, "w": 12, "x": 0, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(app_user_signups_total[5m])) * 60",
          "legendFormat": "Sign-ups per minute",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": { "defaults": { "unit": "short" } }
    },
    {
      "title": "Posts Created Over Time",
      "type": "timeseries",
      "datasource": "Prometheus",
      "id": 5,
      "gridPos": { "h": 10, "w": 12, "x": 12, "y": 6 },
      "targets": [
        {
          "expr": "sum(rate(app_posts_created_total[5m])) * 60",
          "legendFormat": "Posts per minute",
          "refId": "A"
        }
      ],
      "options": {
        "tooltip": { "mode": "multi" },
        "legend": { "displayMode": "list", "placement": "bottom" }
      },
      "fieldConfig": { "defaults": { "unit": "short" } }
    }
  ],
  "refresh": "5m",
  "schemaVersion": 30,
  "style": "dark",
  "tags": ["platform", "business", "kpi"],
  "time": { "from": "now-7d", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Business KPIs",
  "uid": "business-kpis",
  "version": 1
}